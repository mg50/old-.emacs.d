USING: ttt.ui io ;
IN: ttt.ui.consle

TUPLE: console < ui in out ;

: <ui> ( -- ui ) ;

M: print-message ( msg ui -- ) [ stream-writel ] keep stream-flush ;


: coord-string-valid? ( string -- ? )  R/ ^\d+ \d+$/ matches? ;
: parse-coord-string ( string -- x y ) 

M: get-move-coords ( ui -- x y )  ;
