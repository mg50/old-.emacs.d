namespace('Hosemonster.Models', {
    Stream: Backbone.RelationalModel.extend({
        defaults: {
            kind: 'stream',
            'flow-device': new Hosemonster.Models.FlowDevice()
        },

        relations: [{
            type: Backbone.HasOne,
            key: 'flow-device',
            includeInJSON: true,
            relatedModel: Hosemonster.Models.FlowDevice,
            reverseRelation: {
                type: Backbone.HasOne,
                key: 'stream',
                includeInJSON: false
            }
        }],
    })
})
