require 'crud_interactor'
require 'client/client_dbadapter'

module Interactor
  class Client < Lib::CrudInteractor

    def initialize
      @db_adapter = DBAdapter::Client::Singleton
    end

    def find_with_contacts(id, callback)
      client = @db_adapter.find_with_contacts(id)
      callback.call(client)
    end
  end
end
