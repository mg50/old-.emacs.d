(ns server.dispatch.artisan
  (:use [server.dispatch.core :only [def-dispatch-list GET POST ANY*]]))

(def-dispatch-list artisan [request params]
  (GET "\/(.*?)\.txt$"
       :blah)
  (GET ".*"
       [200 (get-in request [:request-line :request-uri])])
  (POST #(and (:)))
  (ANY* ".*"
        [200 "blah"]))
