(ns lisp.core
  (:require [lisp.tokenizer :as tokenizer]
            [lisp.parser :as parser]
            [lisp.env :as env]
            [lisp.evaluator :as evaluator]))

(def global-fns {'+ +})

(def global-env (env/create global-fns [] nil))

(defn interpret [string]
  (-> string
      tokenizer/tokenize
      parser/parse
      (evaluator/eval global-env)))
